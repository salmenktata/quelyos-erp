<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Cron Job : Expirer les réservations dépassées -->
        <record id="ir_cron_expire_reservations" model="ir.cron">
            <field name="name">Stock: Expirer les réservations</field>
            <field name="model_id" ref="model_quelyos_stock_reservation"/>
            <field name="state">code</field>
            <field name="code">model._cron_expire_reservations()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="active" eval="True"/>
        </record>
    </data>
</odoo>
