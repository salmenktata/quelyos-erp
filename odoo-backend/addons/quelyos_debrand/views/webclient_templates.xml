<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <!-- Override layout template for title -->
    <record id="layout_title_override" model="ir.ui.view">
        <field name="name">quelyos.layout.title</field>
        <field name="inherit_id" ref="web.layout"/>
        <field name="priority">100</field>
        <field name="arch" type="xml">
            <xpath expr="//title" position="replace">
                <title><t t-if="title"><t t-esc="title"/> - </t>Quelyos Suite</title>
            </xpath>
        </field>
    </record>

</odoo>
